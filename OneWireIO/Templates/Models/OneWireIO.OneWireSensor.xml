<?xml version="1.0" encoding="iso-8859-1"?>

<Model Name="OneWireIO.OneWireSensor" Version="2.0" Unique="Name">
  <ModelTypeClass>C++</ModelTypeClass>
  <BaseModel>CDPSignalChannel&lt;double&gt;</BaseModel>

  <Attributes>
    <Attribute Name="Model"         Type="string" Required="1" Value="OneWireIO.OneWireSensor" ReadOnly="1"></Attribute>
