    <Attribute Name="SensorID"      Type="string" Description="Sensor ID (dir name found in /sys/bus/w1/devices/) 28-xxxxx..." Required="1"></Attribute>
    <Attribute Name="InternalValue" Type="double" Description="Internal value of CDPChannel" ReadOnly="1"></Attribute>
    <Attribute Name="Value"         Type="double" Description="Value of sensor" Value="0" Required="1"></Attribute>
  </Attributes>
</Model>
